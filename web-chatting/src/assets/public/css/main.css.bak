.select_style li button:hover {background-color:#F6F8FF;}
.check_all {position:relative;}
.check_all input[type="checkbox"]{ position:absolute; left:0; width:0px; height:0px; padding:0; overflow:hidden; clip:rect(0,0,0,0); border:0 none; }
.check_all input[type="checkbox"] + label {position:relative;display:inline-block;width:100%;padding:0 0 0 34px;line-height:24px}
.check_all input[type="checkbox"] + label::before {content:'';position:absolute;left:0;top:0;width:24px;height:24px;background-image:url("../../../../assets/public/public_ext_basic/images/check_deco.png");background-position:left top;background-repeat:no-repeat;}
.check_all input[type="checkbox"]:focus + label::before {outline:0 none;}
.check_all input[type="checkbox"]:checked + label::before {background-position:left -24px;}

/* paging 공통 */
.paging {height:40px;line-height:40px;padding:0 20px;border-top:1px solid #D7DAE0;text-align:center}
.paging_area {margin-top:-2px}
.paging button {height:20px;line-height:19px;margin:0 4px;font-size:14px;font-weight:400;color:#828B99;vertical-align:baseline}
