/* 체크박스 공통 스타일 */
.checkbox_area {position:relative;margin-top:-18px}
.checkbox_area::after {content:"";display:block;clear:both}
.checkbox_area li{float:left;width:33.333%;margin-top:18px}
.checkbox_area li:last-child{margin-right:0;}
/* .checkbox_area li:nth-last-child(-n+3) {margin-bottom:0;} */
.checkbox_area input[type="checkbox"]{ position:absolute; left:0; width:0px; height:0px; padding:0; overflow:hidden; clip:rect(0,0,0,0); border:0 none; }
.checkbox_area input[type="checkbox"] + label{position:relative;display:inline-block;width:100%;padding:1px 0 1px 22px;line-height:1.2}
.checkbox_area input[type="checkbox"] + label:before{content:'';position:absolute;left:0;top:2px;width:17px;height:17px;background-image:url("../../../../assets/public/public_ext_basic/images/setting_check.png");background-position:left top;background-repeat:no-repeat;}
.checkbox_area input[type="checkbox"]:focus + label{outline:1px dotted #444;outline:5px auto -webkit-focus-ring-color;outline:0 none;}
.checkbox_area input[type="checkbox"]:focus + label:before{outline:0 none;}
.checkbox_area input[type="checkbox"]:checked + label:before{background-position:left -17px;}
/* 체크박스 공통 스타일 텍스트 없음 */
.check_deco {position:relative;}
.check_deco input[type="checkbox"] {position:absolute; left:0; width:0px; height:0px; padding:0; overflow:hidden; clip:rect(0,0,0,0); border:0 none;}
.check_deco input[type="checkbox"] + label {position:absolute;right:0;top:0;width:24px;height:24px;background-image:url("../../../../assets/public/public_ext_basic/images/check_deco.png");background-position:left top;background-repeat:no-repeat;text-indent:-9999px;}
.check_deco input[type="checkbox"]:checked + label {background-position:left -24px;}
/* 체크박스 공통 텍스트 체크 */
.check_txt {position:relative;}
.check_txt input[type="checkbox"] { position:absolute; left:-1px; width:0px; height:0px; padding:0; overflow:hidden; clip:rect(0,0,0,0); border:0 none;}
.check_txt input[type="checkbox"] + label {display:block;position:relative;font-size:14px;padding:9px 15px 10px;color:#fff;background-color:#555;font-weight:700;border-radius:3px;z-index:1;cursor:pointer}
.check_txt input[type="checkbox"] + label span {font-size:12px;margin-left:5px}
.check_txt input[type="checkbox"]:checked + label {background-color:#009fe3;}

/* 라디오박스 공통 스타일 */
.radiobox_area {position:relative;margin-top:-11px;overflow:hidden}
.radiobox_area li{display:inline-block;margin-top:15px;margin-right:34px}
.radiobox_area li:last-child{margin-right:0;}
.radiobox_area input[type="radio"]{ position:absolute; left:0; width:0px; height:0px; padding:0; overflow:hidden; clip:rect(0,0,0,0); border:0 none; }
.radiobox_area input[type="radio"] + label{position:relative;display:inline-block;width:100%;padding:1px 0 1px 30px;line-height:1.2;font-weight:400;cursor:pointer;min-height:20px;}
.radiobox_area input[type="radio"] + label:before{content:'';position:absolute;left:0;top:1px;width:20px;height:20px;background-image:url("../../../../assets/public/public_ext_basic/images/setting_radio.png");background-position:left top;background-repeat:no-repeat;}
.radiobox_area input[type="radio"]:focus + label{outline:1px dotted #444;outline:5px auto -webkit-focus-ring-color;outline:0 none;}
.radiobox_area input[type="radio"]:focus + label:before{outline:0 none;}
.radiobox_area input[type="radio"]:checked + label:before{background-position:left -20px;}
.radiobox_area .direct input[type="radio"] + label {width:auto;min-height:30px;}
/* 라디오박스 공통 스타일 네모 스타일 */
.radiobox_area_square {position:relative;margin:-10px 0 0 -10px}
.radiobox_area_square::after {content:"";display:block;clear:both}
.radiobox_area_square li{float:left;width:calc(33.333% - 10px);margin:10px 0 0 10px}
.radiobox_area_square li:last-child{margin-right:0;}
.radiobox_area_square li input[type="radio"] {position:absolute;left:-1px;width:0px;height:0px;padding:0;overflow:hidden;clip:rect(0,0,0,0);border:0 none;}
.radiobox_area_square li input[type="radio"] + label {display:block;position:relative;padding:7px 5px 8px;background-color:#fff;border-radius:5px;border:1px solid #D7DAE0;z-index:1;text-align:center;cursor:pointer}
.radiobox_area_square li input[type="radio"] + label:hover {color:#0012B8;background-color:#F6F8FF;}
.radiobox_area_square li input[type="radio"]:focus + label {outline:0 none;}
.radiobox_area_square li input[type="radio"]:checked + label {color:#0012B8;background-color:#F6F8FF;border-radius:5px;border:1px solid #0012B8;}

