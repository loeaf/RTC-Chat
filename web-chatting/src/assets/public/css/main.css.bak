.menu_view_open {position:fixed;top:0;left:-360px;width:360px;height:100%;background:#fff;-webkit-transition:all 0.3s ease-in-out;transition:all 0.3s ease-in-out;z-index:0;-moz-box-shadow:3px 3px 7px rgba(0,0,0,0.3);-webkit-box-shadow:3px 3px 7px rgba(0,0,0,0.3);box-shadow:3px 3px 7px rgba(0,0,0,0.3);}
