.control_group_r .zoom .zoomIn::after {content:'';position:absolute;left:16px;top:10px;width:2px;height:14px;}
.control_group_r .zoom .zoomOut::before {content:'';position:absolute;left:10px;top:16px;width:14px;height:2px;}
.control_group_r .map_reset button {margin-bottom:36px;background:#fff;text-indent:0;font-size:23px;line-height:35px}
