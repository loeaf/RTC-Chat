/* 분양일정 레이어 */
.sch_pop {position:fixed;transform:translateY(200px);-webkit-transform:translateY(200px);left:50%;bottom:20px;width:624px;height:116px;margin-left:-312px;background:#fff;border-radius:5px;-moz-box-shadow:5px 4px 8px rgba(55, 55, 55, 0.25);-webkit-box-shadow:5px 4px 8px rgba(55, 55, 55, 0.25);box-shadow:5px 4px 8px rgba(55, 55, 55, 0.25);-webkit-transition:all 0.2s;transition:all 0.2s;z-index:9999}
.sch_pop.sch_open {transform:translateY(0);-webkit-transform:translateY(0);-webkit-animation:bounce-in-up 1.2s;animation:bounce-in-up 1.2s;-webkit-animation-timing-function: ease-out;animation-timing-function: ease-out;}
.sch_pop .sch_pop_area {padding:15px 0 0 40px}
.sch_pop .sch_pop_close {position:absolute;top:10px;right:10px;width:30px;height:30px;background:#EDEFF2;border-radius:5px;z-index:1;-webkit-transform:translateZ(0);-webkit-backface-visibility:hidden;}
.sch_pop .sch_pop_close i {font-size:24px;color:#171B27;}
.sch_pop ul::after {content:"";display:block;clear:both}
.sch_pop li {float:left;width:20%;}
.sch_pop li .i_s {position:relative;display:block;width:100%;margin-bottom:10px;}
.sch_pop li .i_s::after {content:"";position:absolute;top:50%;right:0;width:100%;height:2px;margin-top:-1px;background:#EDEFF2;z-index:-1}
.sch_pop li:last-child .i_s::after {width:0;height:0;background:none}
.sch_pop li .i_s i {position:relative;display:inline-block;width:30px;height:30px;line-height:30px;font-size:22px;color:#BDC4D1;background:#D7DAE0;border-radius:100%;text-align:center}
.sch_pop li.end .i_s::after {background:#005DFD}
.sch_pop li.end .i_s i {background:#005DFD}
.sch_pop li.ing .i_s i {background:#0F23B8}
.sch_pop li.ing .i_s i::after {content:"";position:absolute;top:-5px;left:-5px;width:40px;height:40px;background:#fff;border:2px solid #CFD3F1;border-radius:100%;z-index:-1;}
.sch_pop li .date {display:block;font-size:13px;color:#BDC4D1;line-height:1.4}
.sch_pop li .con {color:#BDC4D1;font-weight:700;line-height:1.45}
.sch_pop li.end .date,
.sch_pop li.ing .date {color:#828B99;}
.sch_pop li.ing .con,
.sch_pop li.end .con {color:#171B27;}
@keyframes bounce-in-up{
  0%{transform:translateY(1000px);}
  25%{transform:translateY(50px);}
  30%, 55%, 70%, 80%, 90%, 100%{transform:translateY(0px);}
  45%{transform:translateY(25px);}
  63%{transform:translateY(15px);}
  75%{transform:translateY(7px);}
  85%{transform:translateY(4px);}
  95%{transform:translateY(2px);}
}
@-webkit-keyframes bounce-in-up{
  0%{-webkit-transform: translateY(1000px);}
  25%{-webkit-transform: translateY(50px);}
  30%, 55%, 70%, 80%, 90%, 100%{-webkit-transform:translateY(0px);}
  45%{-webkit-transform:translateY(25px);}
  63%{-webkit-transform:translateY(15px);}
  75%{-webkit-transform:translateY(7px);}
  85%{-webkit-transform:translateY(4px);}
  95%{-webkit-transform:translateY(2px);}
}

