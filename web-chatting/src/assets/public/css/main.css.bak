.setting_view .slider_list li {display:table-cell;text-align:center;}
.setting_view .slider_list li:first-child {text-align:left;}
.setting_view .slider_list li:last-child {text-align:right;}
.setting_view .linkArea {padding:25px 20px;background:#fff}
.hr_style {position:relative;left:-20px;width:calc(100% + 40px);height:10px;background:#EDEFF2;margin-bottom:25px;}
