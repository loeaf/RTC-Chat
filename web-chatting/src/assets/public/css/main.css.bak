/* 메뉴 레이어 공통 닫기 버튼 */
.view_close_area {position:absolute;right:-20px;top:50%;width:20px;height:50px;margin-top:-25px;background-color:#0012B8;border-top-right-radius:5px;border-bottom-right-radius:5px;}
.view_close_area button {position:absolute;left:0;top:0;width:100%;height:100%;text-indent:-9999px;}
.view_close_area button::after {content:"";position:absolute;left:0;top:0;width:100%;height:100%;background:url(../../../../assets/public/public_ext_basic/images/view_close_btn.png) 6px center no-repeat;}
.view_close_area button:hover::after {animation:viewCloseArrow .8s forwards infinite ease;}
/* 닫기 버튼 애니메이션 */
@keyframes viewCloseArrow {0% {background-position:center center;} 40% {background-position:4px center;} 100% {background-position:center center;}}
@-webkit-keyframes viewCloseArrow {0% {background-position:center center;} 40% {background-position:4px center;} 100% {background-position:center center;}}

/* popuplayer 공통 */
.popuplayer {visibility:hidden;overflow:hidden;opacity:0;position:fixed;top:50%;left:50%;width:434px;-webkit-transform:translate(-50%,-50%);-ms-transform:translate(-50%,-50%);transform:translate(-50%,-50%);-webkit-transition:all 0.4s ease-in-out;transition:all 0.4s ease-in-out;z-index:-1;}
.popuplayer.open {visibility:visible;overflow:visible;opacity:1;z-index:9999;}
.popuplayer .popuplayer_area {overflow:hidden;background:transparent;}
.popuplayer .tit {position:relative;height:65px;padding:20px 65px 0 25px;background-color:#fff;}
