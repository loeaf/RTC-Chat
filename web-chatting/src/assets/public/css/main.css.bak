  .land_search_layer .lsl_bottom .linkArea .interest_btn {height:46px;padding:8px;}
  .land_search_layer .lsl_bottom .linkArea a {height:46px;}
  /* lsl_bottom 방문하기 버튼 추가 */
  .land_search_layer .lsl_bottom .linkArea .interest_btn,
  .land_search_layer .lsl_bottom .linkArea.visit_btn_area .interest_btn {height:46px;padding:8px;}

  /* 조건검색 상세 */
  .setting_view .setting_reset {top:21px}
  .result_tag span {margin-right:3px;margin-bottom:3px}
}

/* 420px 이하 */
@media all and (max-width:420px){
  /* input */
  .direct_input .input {width:calc(50% - 15px);float:left;}

  /* 라디오박스 공통 스타일 네모 스타일 */
  .radiobox_area_square li {width:calc(50% - 10px);}

  /* 마이토지 */
  .my_total_info .amount_box .amount .top .txt_right {display:block;margin-top:7px}

  /* 계약하기 팝업 */
  .my_total_info .amount_box .amount.contract_sum .txt_right {display:block;float:none;text-align:left;margin-top:0;}
  .my_total_info .amount_box .amount.contract_sum .bottom .txt_right {margin-top:3px}

  /* nft 상세 페이지 */
  .nft_detail_box .maker_info .cont {padding:7px 20px 9px 20px}
  .nft_detail_box .maker_info .rise,
  .nft_detail_box .maker_info .fall,
  .nft_detail_box .maker_info .zero {top:20px;margin-top:0}
}

/* 400px 이하 */
@media all and (max-width:400px){
  /* 조건검색 상세 */
  .result_tag {height:auto;}
  .setting_view .top_tit_area {height:136px}
  .setting_view .land_search_area.paging_place .scroll_box {height:calc(100% - 136px - 40px)}
  .result_tag span {float:left;}
  .result_tag span:nth-child(3) {max-width:initial;clear:both}
}

/* 380px 이하 */
@media all and (max-width:380px){
  /* 조건검색 상세 */
  /* .result_tag span {max-width:100px;} */

  /* 추천토지 */
  .recommend_area .list {padding:15px;}
  .recommend_area .list .tr {position:relative;bottom:auto;right:auto;display:block;margin-top:6px}
}

/* 360px 이하 */
@media all and (max-width:359px){
  /* 메뉴 레이어팝업 하단 버튼 공통 스타일 rgba(0,18,184,0.8) */
  .linkArea.linkHalf a, .linkArea.linkHalf button {width:calc(50% - 3px) !important;margin-right:6px;font-size:12px}
  /* 에러페이지 */
  .frame_area .error_inner {padding-top:10px}

  /* 조건검색 상세 */
  /* .result_tag span {max-width:60px;} */

  /* 마이타래 */
  .top_tarae .tr,
  .top_tarae .tr span {font-size:25px}
  /* 마이토지 */
  .asset_tab li button {font-size:13px}
  .asset_tab li.active button::after {bottom:-4px}
}

/* 350px 이하 */
@media all and (max-width:350px){
  .login_layer_right .my_login_info .my_info {display:none}
}

/* 330px 이하 */
@media all and (max-width:330px){
  /* nft상세 */
  .nft_detail_box .maker_info .cont {padding:7px 10px 9px 10px}
  .nft_detail_box .maker_info .rise, .nft_detail_box .maker_info .fall, .nft_detail_box .maker_info .zero {top:-1px;right:-1px}
}

/* 높이 600px 이하 */
@media all and (max-height:600px){
  /* 메뉴 */
  #nav {height:calc(100% - 63px - 63px);overflow-y:auto;}
  #nav::-webkit-scrollbar {position:absolute;top:0;right:0;width:7px;margin-right:10px} /* 스크롤바의 너비 */
  #nav::-webkit-scrollbar-thumb {background-color:#061437;} /* 스크롤바의 색상 */
  #nav::-webkit-scrollbar-track {background-color:transparent; margin-right:15px} /* 스크롤바 트랙 색상 */

  .r_map_list .cityStyle_layer {height:202px;overflow-y:auto}
}

/* 높이 380px 이하 */
@media all and (max-height:380px){
  .control_group_r .map_reset,
  .control_group_r .zoom {display:none}
}
