@charset "utf-8";

html, body, #wrap {height:100%;width:100%;}
body {overflow:hidden;touch-action:pan-x;touch-action:pan-y;position:fixed}
/* body {overflow:hidden;height:100vh;height:calc(var(--vh, 1vh) * 100);height:calc(100vh - 80px);height:calc(var(--vh, 1vh) * 100 - 80px);} */

#wrap {position:relative;overflow:hidden;}
a {cursor:pointer}
.red {color:#ff3c00;font-size:inherit;font-weight:inherit;}
.status {float:right;padding:3px 5px;line-height:1.3;font-size:13px;color:#FF5857;border:1px solid #FF5857;}
.status.complete {color:#828B99;border:1px solid #828B99}
.status.possess {color:#225FF5;border:1px solid #225FF5}

/* mask */
.mask {display:none;position:fixed;width:100%;height:100%;left:0;top:0;background-color:rgba(33,34,36,0.5);z-index:-1;-webkit-transition:all 0.4s;transition:all 0.4s;}
.layerBg .mask {display:block;z-index:9998;}
.layerBg.maskNo .mask {display:none;z-index:-1;}
.layerBg.maskOn .mask {display:block;z-index:999;}

/* 툴팁 */
#powerTip {display:none;cursor:default;position:absolute;font-size:12px;color:#0A1E50;/*margin-left:5px;*/background-color:#fff;border-radius:5px;padding:5px 10px 6px 10px;white-space:nowrap;font-weight:700;z-index:2147483647;-moz-box-shadow:3px 1px 10px rgba(62, 66, 104, 0.3);-webkit-box-shadow:3px 1px 10px rgba(62, 66, 104, 0.3);box-shadow:3px 1px 10px rgba(62, 66, 104, 0.3)}
.top_color #powerTip {color:#fff;background:#0A1E50;-moz-box-shadow:0 0 0 transparent;-webkit-box-shadow:0 0 0 transparent;box-shadow:0 0 0 transparent}

/* dialogBox */
/* .dialogClick {position:absolute;left:80px;bottom:20px;color:#fff;z-index:1} */
.dialogBox,
.browser_check_box,
.device_1024 {display:none;cursor:default;position:fixed;left:50%;bottom:30px;width:860px;background:#F6F8FF;border:1px solid #005DFD;border-radius:5px;z-index:2147483647;-moz-box-shadow:3px 1px 10px rgba(62, 66, 104, 0.3);-webkit-box-shadow:3px 1px 10px rgba(62, 66, 104, 0.3);box-shadow:3px 1px 10px rgba(62, 66, 104, 0.3);-webkit-transform:translateX(-50%);-ms-transform:translateX(-50%);transform:translateX(-50%);letter-spacing:0px;text-align:center}
.dialogBox p,
.browser_check_box p,
.device_1024 p {padding:13px 25px 14px;font-size:14px;color:#171B27;font-weight:700;line-height:1.5;word-break:keep-all;/* word-break:break-all; */}
.dialogBox.info {background:#D9F9F5;border:1px solid #07CAB5;}/* 정보내용(초록) */
.dialogBox.warning {background:#FFF4D0;border:1px solid #FFCA12;}/* 경고내용(노랑) */
.dialogBox.error {background:#FFE9E9;border:1px solid #FF5857;}/* 오류내용(빨강) */
.browser_check_box {display:none;width:800px;bottom:auto;top:30px;background:#061437;border:1px solid #061437;border-radius:10px;z-index:9999}
.browser_check_box.not_chrome {display:block;padding-right:40px;}
.browser_check_box p {color:#fff;font-weight:500;text-align:center;padding-right:0 !important}
.browser_check_box .close {position:absolute;top:13px;right:20px}
.browser_check_box .close i {color:#fff;font-size:22px}

/* map_dialog */
.map_dialog {position:absolute;left:50%;bottom:50%;min-width:80px;border:1px solid #0F23B8;border-radius:10px;text-align:center;overflow:hidden;z-index:1}
.map_dialog span {display:inline-block;}
.map_dialog .top {padding:4px 10px;background:#fff}
.map_dialog .bottom {padding:6px 15px;background:#fff;background:#0F23B8;}
.map_dialog .top .tit {font-size:13px;color:#5C6575;line-height:1.4;letter-spacing:-0.5px;}
.map_dialog .bottom .tit {margin-right:6px;font-size:11px;color:#D7DAE0;line-height:1.4;vertical-align:middle;letter-spacing:-0.5px;}
.map_dialog .num {font-size:15px;color:#fff;line-height:1.4;vertical-align:middle;letter-spacing:-0.5px;}

/* input */
input {width:100%;height:28px;line-height:28px;padding:0 8px;border:1px solid #D7DAE0;border-radius:5px;}
input:focus {outline:1px solid #005dfd;border-radius:5px}
input[type="text"], input[type="password"], input[type="email"], input[type="search"], input[type="image"], input[type="tel"], textarea {appearance:none;-moz-appearance:none;-webkit-appearance:none;border-radius:0;-webkit-border-radius:0;-moz-border-radius:0;}
.direct_input {display:inline-block;margin-left:10px;vertical-align:middle;}
.direct_input::after {content:"";display:block;clear:both}
.direct_input .input {width:85px;float:left;}
.direct_input i {float:left;line-height:28px;margin:0 8px;color:#828B99;}

/* full 로딩 */
.full_loading {display:none;position:fixed;top:0;left:0;width:100%;height:100%;background:rgba(33,34,36,0.5);z-index:2000;text-align:center}
.full_loading.active {display:block}
.full_loading .img img {position:absolute;left:50%;top:50%;width:auto;height:120px;margin-top:-60px;opacity:0.8;-webkit-transform:translateX(-50%);-ms-transform:translateX(-50%);transform:translateX(-50%);}

/* 목록 로딩 */
.loading_list {position:absolute;top:0;left:0;width:100%;height:100%;text-align:center;overflow-y:auto}
.loading_list img {position:absolute;left:50%;top:50%;width:120px;height:120px;margin-top:-60px;-webkit-transform:translateX(-50%);-ms-transform:translateX(-50%);transform:translateX(-50%);}
/* .loading_list .vaiv_ani_1 {width:auto;height:44px;margin-top:-22px;}
.loading_list .vaiv_ani_2 {width:auto;height:90px;margin-top:-45px;} */

/* 호버시 팁 영역 공통 */
.tip_area {position:relative;display:inline-block;}
.tip_area .tip_btn,
.tip_area .tip_btn_2 {font-size:22px;color:#BDC4D1;}
/* .tip_area .tip_btn_2 {margin:1px 0 0 4px;width:17px;height:17px;background-image:url(../../../../assets/public/public_ext_basic/images/icon_auestion_gray.png);background-position:center;background-repeat:no-repeat;text-indent:-9999px;}
.tip_area .tip_btn_2.exclamation_btn {background-image:url(../../../../assets/public/public_ext_basic/images/icon_exclamation_gray.png);} */
.tip_area .tip_box {visibility:hidden;overflow:hidden;opacity:0;position:fixed;width:236px;margin-left:-118px;padding:10px 30px;font-size:13px;color:#fff;background:#0A1E50;border-radius:5px;/* -moz-box-shadow:3px 3px 7px rgba(0,0,0,0.1);-webkit-box-shadow:3px 3px 7px rgba(0,0,0,0.1); */}
.tip_area.box_fixed .tip_box {visibility:visible;overflow:visible;opacity:1;z-index:10;}
.tip_area .tip_box::after {content:"";display:block;position:absolute;top:-10px;left:50%;width:22px;height:11px;margin-left:-11px;background:url(../../../../assets/public/public_ext_basic/images/icon_tip_top_arrow.png) center no-repeat;}
.tip_area .tip_box strong {display:block;margin-bottom:4px;font-size:14px !important;color:#fff !important;font-weight:700 !important}
.tip_area .tip_box p {font-weight:400;line-height:20px;word-break:keep-all}
.tip_area .tip_box.box_right {top:31px;margin-left:-224px;padding-right:14px}
.tip_area .tip_box.box_right::after {left:auto;right:16px;margin-left:0;}
.tip_area .tip_box.arrow_left_center {margin:-51px 0 0 40px}
.tip_area .tip_box.arrow_left_center::after {top:50%;left:-15px;margin-left:0;margin-top:-6px;-webkit-transform:rotate(90deg);transform:rotate(90deg)}
.tip_area .tip_box.arrow_left_bottom {margin:-60px 0 0 40px;}
.tip_area .tip_box.arrow_left_bottom::after {top:auto;bottom:19px;left:-15px;margin-left:0;-webkit-transform:rotate(90deg);transform:rotate(90deg)}
.tip_area .tip_box.box_top {margin:-103px 0 0 -100px;text-align:center}
.tip_area .tip_box.box_top::after {top:auto;bottom:-8px;margin-left:-12px;}

/* 스크롤 */
/* .search_view .mCSB_container,
.interest_view .mCSB_container {height:100%;} */
.scroll_box {position:relative;width:100%;height:100%;overflow-y:auto;overflow-x:hidden; scrollbar-color:#BDC4D1 transparent;}
.scroll_box::-webkit-scrollbar {position:absolute;top:0;right:0;width:7px;margin-right:10px} /* 스크롤바의 너비 */
.scroll_box::-webkit-scrollbar-thumb {background-color:#BDC4D1;} /* 스크롤바의 색상 */
.scroll_box::-webkit-scrollbar-track {background-color:#EDEFF2;margin-right:15px} /* 스크롤바 트랙 색상 */
.scroll_x {position:relative;width:100%;height:100%;overflow-y:auto;overflow-x:hidden; scrollbar-color:#BDC4D1 transparent;}
.scroll_x::-webkit-scrollbar {position:absolute;top:0;right:0;width:7px;} /* 스크롤바의 너비 */
.scroll_x::-webkit-scrollbar-thumb {background-color:#BDC4D1;} /* 스크롤바의 색상 */
.scroll_x::-webkit-scrollbar-track {background-color:#EDEFF2;} /* 스크롤바 트랙 색상 */
.scroll_x::-webkit-scrollbar:horizontal {height:8px;}/* 가로 스크롤 높이 */
#wrap .mCSB_inside > .mCSB_container {margin-right:0;}
#wrap .mCSB_scrollTools {top:10px;right:4px;bottom:10px;width:4px;opacity:1;z-index:2;}
#wrap .mCSB_scrollTools .mCSB_dragger .mCSB_dragger_bar,
#wrap .mCSB_scrollTools .mCSB_dragger:hover .mCSB_dragger_bar,
#wrap .mCSB_scrollTools .mCSB_dragger:active .mCSB_dragger_bar,
#wrap .mCSB_scrollTools .mCSB_dragger.mCSB_dragger_onDrag .mCSB_dragger_bar {background-color:#a0a0a0;border-radius:2px;}
#wrap .mCSB_scrollTools .mCSB_draggerRail {width:4px;/* background-color:#d5d5d5; */background-color:#e7e7e7;border-radius:0;}

/* header */
#header {position:fixed; left:0; top:0; width:63px; height:100%;background-color:#101E4D; z-index:1000;}
.layerBg #header {z-index:999;}
#header .logo {position:absolute;left:0;top:0;width:100%;height:63px;line-height:63px;text-align:center;border-bottom:1px solid rgba(255,255,255,0.5);background:#101E4D url(../../../../assets/public/public_ext_basic/images/logo_bg.png) center no-repeat;}
#header .logo img {width:25px;height:26px;vertical-align:middle;}
#nav {position:relative; margin-top:63px;}
#nav li {text-align:center;}
#nav li a {position:relative;display:block;padding:20px 0;color:#fff;}
#nav li:first-child a {padding-top:72px}
#nav li a i {font-size:24px;color:rgba(255,255,255,0.6);}
#nav li a span {display:block;padding-top:12px;font-size:12px;color:rgba(255,255,255,0.6);}
#nav li.nav_market a i {display:inline-block;width:26px;height:26px;background:url(../../../../assets/public/public_ext_basic/images/icon_market.png) center no-repeat;background-size:26px 26px;}
#nav li.active a span,
#nav li.active a i,
#nav li a:hover i,
#nav li a:hover span {color:#fff}
#nav li.active.nav_market a i,
#nav li.nav_market:hover a i {background:url(../../../../assets/public/public_ext_basic/images/icon_market_on.png) center no-repeat;background-size:26px 26px;}
#nav li.active a {}
#nav li.active a::after {content:"";position:absolute;bottom:36px;right:0;width:3px;height:39px;background:url(../../../../assets/public/public_ext_basic/images/nav_active_bg.png) center no-repeat;}
/* #nav li.active.nav_search a i::before {content:"\EF13"} */
#nav li.active.nav_setting a i::before {content:"\EC9C"}
#nav li.active.nav_interest a i::before {content:"\F186"}
#nav li.active.nav_myLand a i::before,
#nav li.active.nav_asset a i::before {content:"\EF19"}
#nav li.active.nav_more a i::before {content:"\EF78"}

#nav li .noti_icon {position:relative;display:inline-block;padding-top:0;font-size:inherit;}
#nav li .noti_icon::after {content:"";display:block;position:absolute;top:-3px;right:-7px;width:5px;height:5px;border-radius:100%;background:#D7FF3C}

#header .service_info {position:absolute;left:0;bottom:0;width:100%;margin-bottom:10px;}
#header .service_info .tip_area {width:100%;}
#header .service_info .tip_area .tip_box.arrow_left_center {margin:-54px 0 0 65px}
#header .service_info button {display:block;/* width:100%; */padding:10px 0;font-size:12px;color:#fff;margin:0 auto;height:auto;background-image:none;text-indent:0;}
#header .service_info i {color:#fff;font-size:22px}
#header .service_info button span {display:block;padding-top:10px;font-size:12px;}
/* #header .service_info button:hover span {color:#009fe3} */

/* container */
#container {position:relative;width:100%;height:100%;z-index:1;}

/* no_data */
.no_data {display:none;position:absolute;top:0;left:0;width:100%;height:100%;margin:0;padding:0;background:none;z-index:-1;}
.no_data_on .no_data {display:table;z-index:10}
.no_data .no_data_cell {display:table-cell;padding:20px;background:#fff;vertical-align:middle;text-align:center}
.no_data .no_data_cell .tit {display:block;line-height:1.4;color:#5C6575;}
.no_data .no_data_cell .txt {margin-top:10px;font-size:13px;color:#828B99;line-height:1.4;font-weight:400;word-break:keep-all}
.asset_view .asset_view_area .no_data_on .no_data {height:100%}
.asset_view .asset_view_area .no_data_on.asset_cont04 .no_data {margin-top:0}

/* finish_box */
.finish_box {padding:0 20px 20px;text-align:center;background-color:#fff;}
.finish_box .finish_tit {display:block;font-size:19px;padding:0;font-weight:900;}
.finish_box .finish_txt {font-size:19px;color:#061437;line-height:1.5;word-break:keep-all}
.finish_box .finish_txt strong {font-weight:900;}
.finish_box .finish_txt2,
.finish_box .finish_txt3 {margin-top:15px;font-weight:400;line-height:1.5;}
.finish_box .finish_txt2 {font-size:15px;color:#5C6575; }
.finish_box .finish_txt3 {font-size:18px;color:#061437;}
.finish_box .finish_ul {margin-top:20px;}
.finish_box .finish_ul li {position:relative;padding-left:12px;margin-bottom:3px;font-size:13px;font-weight:400;color:#5C6575;text-align:left;word-break:keep-all}
.finish_box .finish_ul li:last-child,
.finish_box .finish_ul.finish_ul_s2 li:last-child {margin-bottom:0}
.finish_box .finish_ul li span {position:absolute;left:0;top:0;}
.finish_box .finish_ul li .mini {font-size:11px}
.finish_box .finish_ul.finish_ul_s2 li {font-size:15px;margin-bottom:15px}

/* 로그인 */
.login_box {position:absolute;right:15px;top:20px;text-align:center;z-index:3;-webkit-transition:all 0.3s;transition:all 0.3s;}
.login_box button {width:100%;height:100%;color:#fff;font-size:12px;}
.login_box i {display:block;margin:0 auto;background-position:center;background-repeat:no-repeat;vertical-align:sub}
.login_box .login,
.my_info {width:60px;height:60px;border-radius:100%;overflow:hidden;}
.login_box .login button {padding:9px 10px 9px 9px;background:#232933}
.login_box .login i {color:#fff;font-size:30px}
.login_box.active .my_info {margin-right:190px;-webkit-transition:all 0.3s;transition:all 0.3s;}
.my_info {position:relative;z-index:1;-webkit-transition:all 0.3s;transition:all 0.3s;}
.my_info button {display:block;width:100%;height:100%;background-color:#009fe3;color:#fff}
.my_info button img {width:100%;height:100%;background:transparent;}
.my_info .my_id {display:block;width:100%;padding:0;font-size:19px;font-weight:500;word-break:break-all;letter-spacing:-0.5px}
.my_info_open_box {position:absolute;top:4px;right:4px;width:52px;height:52px;border-radius:50px;-webkit-transition:all 0.3s;transition:all 0.3s;overflow:hidden;border:1px solid #D7DAE0;background:#fff;}
.my_info_open_box p {position:relative;width:242px;padding:15px 25px;text-align:right;}
.my_info_open_box p::after {content:"";display:block;position:absolute;left:5px;top:0;width:100%;height:100%;z-index:-1;}
.my_info_open_box a {position:relative;padding-left:30px;font-size:14px;color:#171B27;font-weight:400;line-height:1.45;}
.my_info_open_box a::after {content:"";display:block;position:absolute;left:11px;top:50%;width:5px;height:5px;margin-top:-2.5px;background:#D7DAE0;border-radius:50%;}
.login_box.active .my_info_open_box {width:242px;-webkit-transition:all 0.3s;transition:all 0.3s;}

/* control_group_r */
.control_group_r {position:absolute;right:20px;z-index:3;-webkit-transition:all 0.3s ease-in-out;transition:all 0.3s ease-in-out;}
.details_open .control_group_r {right:415px;}
.wallet_open .control_group_r {right:390px;}
.control_group_r {bottom:30px;width:34px;}
.control_group_r .zoom button,
.control_group_r button,
.controller li button {position:relative;width:34px;height:34px;border-radius:5px;overflow:hidden;text-indent:-9999px;margin-bottom:10px;background-color:#0c3980;-webkit-transition:all 0.2s;transition:all 0.2s;}
.control_group_r .zoom button {background:#fff;/* background:rgba(255,255,255,0.9);-moz-box-shadow:3px 3px 5px rgba(0,0,0,0.2);-webkit-box-shadow:3px 3px 5px rgba(0,0,0,0.2);box-shadow:3px 3px 5px rgba(0,0,0,0.2) */}
