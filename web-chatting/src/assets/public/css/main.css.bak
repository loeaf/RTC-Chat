.popuplayer .close::before {position:absolute;right:0;top:0;width:100%;height:100%;line-height:30px;text-indent:0;font-size:24px;color:#171B27}
.popuplayer .popuplayer_group {position:relative;width:100%;background-color:#fff;}
.popuplayer .txt {color:#484848; line-height:24px}
