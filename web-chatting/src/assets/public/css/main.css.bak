.select_box select  {padding-right:22px; padding-bottom:4px; font-size:14px; font-weight:500; letter-spacing:-0.5px; border:0 none; background:#e7e7e7 url(../../../../assets/public/public_ext_basic/images/icon_select_gray.png) right 4px no-repeat; outline:none;}
/* select 스타일의 a,button */
.select_style {position:relative;font-weight:500;letter-spacing:-0.5px;border:0 none; background:#e7e7e7;}
