.paging button:hover,
.paging button:focus {color:#171B27;}
.paging .first,
.paging .prev,
.paging .next,
.paging .last {width:20px;margin:0;background-image:url(../../../../assets/public/public_ext_basic/images/search_view_paging.png);background-position:left top;background-repeat:no-repeat;text-indent:-9999px}
.paging .first {margin-right:-3px;background-position:left top;}
.paging .prev {margin-right:4px;background-position:-20px top;}
.paging .next {margin-left:4px;background-position:-40px top;}
.paging .last {margin-left:-3px;background-position:-60px top;}
.paging .first.disabled {background-position:left -20px;}
.paging .prev.disabled {background-position:-20px -20px;}
.paging .next.disabled {background-position:-40px -20px;}
.paging .last.disabled {background-position:-60px -20px;}
