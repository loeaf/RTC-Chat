.select_style .label::after {content:'';display:inline-block;position:relative;top:4px;width:10px;height:6px;margin:0 0 0 7px;vertical-align:top;-webkit-transition:all 0.2s;transition:all 0.2s;background:url(../../../../assets/public/public_ext_basic/images/icon_select_gray.png) center no-repeat;}
